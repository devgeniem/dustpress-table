<table>
    <thead>
        <tr>
            {#columns}
                <th data-key="{key}">{label}</th>
            {/columns}
        </tr>
    </thead>
    <tbody>
        {#data}
            <tr>
                {>"{tableId}Row" /}
            </tr>
        {/data}
    </tbody>
</table>
{#pagination}
    {>"pagination" /}
{/pagination}
